{
  "name": "site",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "build": "./node_modules/.bin/webpack --config ./webpack.config.prod.ts || true",
    "static": "npm run build && ./node_modules/.bin/webpack --config ./webpack.config.pre.ts || true && rm recipes tagCol tags || true && node ./webpackDist/prerender.js",
    "dev": "cross-env ./node_modules/.bin/webpack-dev-server --config ./webpack.config.ts --port 4201 --content-base webpackDist"
  },
  "dependencies": {
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@xpfw/data": "1.0.0",
    "@xpfw/data-bulma": "1.0.0",
    "@xpfw/data-feathers": "^1.0.0",
    "@xpfw/data-nedb": "1.0.0",
    "@xpfw/dm": "1.0.0",
    "@xpfw/dm-shared": "1.0.0",
    "@xpfw/form": "1.0.0",
    "@xpfw/form-bulma": "1.0.0",
    "@xpfw/form-tests": "1.0.0",
    "@xpfw/form-web": "1.0.0",
    "@xpfw/router": "1.0.0",
    "@xpfw/test-util": "1.0.0",
    "ajv": "^6.12.2",
    "awesome-typescript-loader": "^5.2.1",
    "bulma": "^0.8.2",
    "fast-json-patch": "^3.0.0-1",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "moment": "^2.24.0",
    "nedb": "^1.8.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-syntax-highlighter": "^12.2.1",
    "victory": "^34.1.3"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/graceful-fs": "^4.1.3",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.4",
    "@types/npm": "^2.0.31",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-test-renderer": "^16.9.2",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.41.12",
    "@xpfw/data-tests": "^1.0.0",
    "@xpfw/test-util": "1.0.0",
    "babel-runtime": "^6.26.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "graceful-fs": "^4.2.4",
    "jest": "^25.5.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.0",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "ts-jest": "=25.4.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(css|sass|scss)$": "<rootDir>/__mocks__/styleMock.ts",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
      "isofw-node(.*)": "<rootDir>/../isofw-node$1",
      "isofw-shared(.*)": "<rootDir>/../isofw-shared$1",
      "isofw-web(.*)": "<rootDir>/../isofw-web$1",
      "isofw-rn(.*)": "<rootDir>/../isofw-rn$1",
      "@xpfw/form": "<rootDir>/node_modules/@xpfw/form",
      "@xpfw/data": "<rootDir>/node_modules/@xpfw/data",
      "@xpfw/ui-tests": "<rootDir>/node_modules/@xpfw/ui-tests",
      "@xpfw/test-util": "<rootDir>/node_modules/@xpfw/test-util"
    },
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
