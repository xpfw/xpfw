{
  "name": "@xpfw/router",
  "version": "1.0.0-beta.14",
  "main": "dist",
  "typings": "dist",
  "license": "MIT",
  "dependencies": {
    "mobx": "^5.9.0",
    "mobx-react-lite": "^1.2.0"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "dev": "./node_modules/.bin/rollup -c -w",
    "test": "./node_modules/.bin/jest --ci",
    "update": "ncu -a",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.8",
    "@types/react-test-renderer": "^16.8.1",
    "jest": "^24.5.0",
    "jest-cli": "^24.5.0",
    "react": "^16.8.5",
    "react-test-renderer": "^16.8.5",
    "rollup": "^1.7.3",
    "rollup-plugin-typescript2": "^0.20.1",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "=24.0.0",
    "tslib": "^1.9.3",
    "typescript": "^3.3.4000",
    "uglify-es": "^3.3.10"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {},
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testURL": "http://localhost/"
  }
}
