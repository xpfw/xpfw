{
  "name": "@xpfw/router",
  "version": "1.0.0",
  "main": "dist",
  "typings": "dist",
  "license": "MIT",
  "dependencies": {
    "mobx": "^5.15.1",
    "mobx-react-lite": "^1.5.2"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "dev": "./node_modules/.bin/rollup -c -w",
    "test": "./node_modules/.bin/jest --ci",
    "update": "ncu -a",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/react": "^16.9.17",
    "@types/react-test-renderer": "^16.9.1",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "react": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "rollup": "^1.27.14",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "=24.2.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.4",
    "uglify-es": "^3.3.10"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {},
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testURL": "http://localhost/"
  }
}
