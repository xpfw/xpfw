{
  "name": "@xpfw/form-bulma",
  "version": "1.0.0-beta.16",
  "main": "dist",
  "typings": "dist",
  "license": "MIT",
  "peerDependencies": {
    "@xpfw/form": "1.x.x",
    "@xpfw/form-web": "1.x.x",
    "lodash": "4.x.x",
    "mobx": "^5.x.x",
    "mobx-react-lite": "^1.x.x",
    "moment": "2.x.x"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "dev": "./node_modules/.bin/rollup -c -w",
    "test": "./node_modules/.bin/jest --ci",
    "storybook": "./node_modules/.bin/start-storybook -p 9001 -c .storybook",
    "update": "ncu -a",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^11.11.6",
    "@types/react": "^16.8.8",
    "@types/react-test-renderer": "^16.8.1",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__react": "^4.0.1",
    "@xpfw/form": "1.0.0-beta.16",
    "@xpfw/form-tests": "1.0.0-beta.16",
    "@xpfw/form-web": "1.0.0-beta.16",
    "awesome-typescript-loader": "^5.2.1",
    "jest": "^24.5.0",
    "jest-cli": "^24.5.0",
    "lodash": "^4.17.11",
    "mobx": "^5.9.0",
    "mobx-react-lite": "^1.1.1",
    "mockdate": "^2.0.2",
    "moment": "^2.24.0",
    "react": "^16.8.5",
    "react-test-renderer": "^16.8.5",
    "rollup": "^1.7.3",
    "rollup-plugin-typescript2": "^0.20.1",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "=24.0.0",
    "typescript": "^3.3.4000",
    "uglify-es": "^3.3.10"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "^@xpfw/form-tests$": "<rootDir>/node_modules/@xpfw/form-tests",
      "^@xpfw/form-web$": "<rootDir>/node_modules/@xpfw/form-web",
      "^@xpfw/form$": "<rootDir>/node_modules/@xpfw/form",
      "^react$": "<rootDir>/node_modules/react"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testURL": "http://localhost/"
  }
}
