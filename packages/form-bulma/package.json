{
  "name": "@xpfw/form-bulma",
  "version": "1.0.1",
  "main": "dist",
  "typings": "dist",
  "license": "MIT",
  "peerDependencies": {
    "@xpfw/form": "1.x.x",
    "@xpfw/form-web": "1.x.x",
    "lodash": "4.x.x",
    "mobx": "^6.x.x",
    "mobx-react": "^7.x.x",
    "moment": "2.x.x"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "dev": "./node_modules/.bin/rollup -c -w",
    "test": "./node_modules/.bin/jest --ci",
    "update": "ncu -a",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "@types/react-test-renderer": "^17.0.1",
    "@xpfw/form": "1.0.1",
    "@xpfw/form-tests": "1.0.1",
    "@xpfw/form-web": "1.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lodash": "^4.17.20",
    "mobx": "^6.1.6",
    "mobx-react": "^7.1.0",
    "mockdate": "^3.0.2",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "rollup": "^2.39.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "=26.5.1",
    "typescript": "^4.1.5",
    "uglify-es": "^3.3.10"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "^@xpfw/form-tests$": "<rootDir>/node_modules/@xpfw/form-tests",
      "^@xpfw/form-web$": "<rootDir>/node_modules/@xpfw/form-web",
      "^@xpfw/form$": "<rootDir>/node_modules/@xpfw/form",
      "^react$": "<rootDir>/node_modules/react",
      "^moment$": "<rootDir>/node_modules/moment"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testURL": "http://localhost/"
  }
}
