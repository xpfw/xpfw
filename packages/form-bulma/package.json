{
  "name": "@xpfw/form-bulma",
  "version": "1.0.0",
  "main": "dist",
  "typings": "dist",
  "license": "MIT",
  "peerDependencies": {
    "@xpfw/form": "1.x.x",
    "@xpfw/form-web": "1.x.x",
    "lodash": "4.x.x",
    "mobx": "^5.x.x",
    "mobx-react": "^6.x.x",
    "moment": "2.x.x"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "dev": "./node_modules/.bin/rollup -c -w",
    "test": "./node_modules/.bin/jest --ci",
    "update": "ncu -a",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/react-test-renderer": "^16.9.2",
    "@xpfw/form": "1.0.0",
    "@xpfw/form-tests": "1.0.0",
    "@xpfw/form-web": "1.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "jest": "^25.5.2",
    "jest-cli": "^25.5.2",
    "lodash": "^4.17.11",
    "mobx": "^5.9.0",
    "mobx-react": "^6.1.8",
    "mockdate": "^2.0.5",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rollup": "^2.7.5",
    "rollup-plugin-typescript2": "^0.27.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "=25.4.0",
    "typescript": "^3.8.3",
    "uglify-es": "^3.3.10"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "^@xpfw/form-tests$": "<rootDir>/node_modules/@xpfw/form-tests",
      "^@xpfw/form-web$": "<rootDir>/node_modules/@xpfw/form-web",
      "^@xpfw/form$": "<rootDir>/node_modules/@xpfw/form",
      "^react$": "<rootDir>/node_modules/react"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testURL": "http://localhost/"
  }
}
