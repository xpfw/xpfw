{
  "name": "@xpfw/form-web-location",
  "version": "0.1.1",
  "main": "dist",
  "typings": "dist",
  "license": "MIT",
  "peerDependencies": {
    "lodash": "4.x.x",
    "moment": "2.x.x",
    "leaflet": "1.x.x",
    "@xpfw/form": "1.x.x",
    "mobx": "^5.x.x",
    "mobx-react-lite": "^1.x.x"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "dev": "./node_modules/.bin/rollup -c -w",
    "test": "./node_modules/.bin/jest --ci",
    "storybook": "./node_modules/.bin/start-storybook -p 9001 -c .storybook",
    "update": "ncu -a",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/leaflet": "^1.4.4",
    "@types/lodash": "^4.14.123",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^11.13.9",
    "@types/react": "^16.8.15",
    "@xpfw/form": "1.0.0-beta.17",
    "@xpfw/form-tests": "1.0.0-beta.17",
    "@xpfw/validate": "0.1.2",
    "awesome-typescript-loader": "^5.2.1",
    "jest": "^24.7.1",
    "jest-cli": "^24.7.1",
    "leaflet": "^1.4.0",
    "lodash": "^4.17.11",
    "mobx-react-lite": "^1.1.1",
    "mobx": "^5.9.0",
    "mockdate": "^2.0.2",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "rollup": "^1.11.0",
    "rollup-plugin-typescript2": "^0.21.0",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "=24.0.2",
    "typescript": "^3.4.5",
    "uglify-es": "^3.3.10"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "\\.(css|sass|scss)$": "<rootDir>/__mocks__/styleMock.ts",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
      "^@xpfw/form-tests$": "<rootDir>/node_modules/@xpfw/form-tests",
      "^@xpfw/form$": "<rootDir>/node_modules/@xpfw/form",
      "^react$": "<rootDir>/node_modules/react"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testURL": "http://localhost/"
  }
}
