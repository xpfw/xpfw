{
  "name": "@xpfw/data-feathers",
  "version": "1.0.0-beta.8",
  "main": "dist",
  "typings": "dist",
  "license": "MIT",
  "dependencies": {
    "@feathersjs/authentication-client": "^1.0.10",
    "@feathersjs/feathers": "^3.3.1",
    "@feathersjs/rest-client": "^1.4.7",
    "@feathersjs/socketio-client": "^1.2.1",
    "socket.io-client": "^2.2.0"
  },
  "peerDependencies": {
    "lodash": "4.x.x",
    "moment": "2.x.x",
    "@xpfw/form": "1.x.x",
    "@xpfw/data": "1.x.x"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "dev": "./node_modules/.bin/rollup -c -w",
    "test": "./node_modules/.bin/jest --ci",
    "update": "ncu -a",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "lodash-es": "^4.17.11",
    "mobx": "^5.9.0",
    "ajv": "^6.9.1",
    "@types/feathersjs__authentication-client": "^1.0.4",
    "@types/feathersjs__feathers": "^3.1.1",
    "@types/feathersjs__rest-client": "^1.3.0",
    "@types/feathersjs__socketio-client": "^1.0.1",
    "@types/jest": "^23.3.12",
    "@types/lodash": "^4.14.119",
    "@types/lodash-es": "^4.14.118",
    "@types/mockdate": "^2.0.0",
    "@types/socket.io-client": "^1.4.32",
    "@xpfw/form": "^1.0.0-beta.8",
    "@xpfw/test-util": "1.0.0-beta.8",
    "@xpfw/data": "^1.0.0-beta.8",
    "@xpfw/data-tests": "1.0.0-beta.8",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "lodash": "^4.17.11",
    "mockdate": "^2.0.2",
    "moment": "^2.23.0",
    "react": "^16.7.0",
    "rollup": "^1.1.0",
    "rollup-plugin-typescript2": "^0.18.1",
    "rollup-plugin-uglify": "^6.0.0",
    "ts-jest": "=23.10.5",
    "typescript": "^3.2.2",
    "uglify-es": "^3.3.10"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "@xpfw/validate": "<rootDir>/node_modules/@xpfw/validate",
      "@xpfw/form-shared": "<rootDir>/node_modules/@xpfw/form-shared",
      "@xpfw/ui-shared": "<rootDir>/node_modules/@xpfw/ui-shared",
      "react": "<rootDir>/node_modules/react"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testURL": "http://localhost/"
  }
}
