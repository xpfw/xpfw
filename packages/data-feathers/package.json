{
  "name": "@xpfw/data-feathers",
  "version": "1.0.0",
  "main": "dist",
  "typings": "dist",
  "license": "MIT",
  "dependencies": {
    "@feathersjs/authentication-client": "^4.4.3",
    "@feathersjs/feathers": "^4.4.3",
