    "mobx": "^5.15.1",
    "mockdate": "^2.0.5",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-is": "^16.12.0",
    "rollup": "^1.27.14",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-uglify": "^6.0.4",
