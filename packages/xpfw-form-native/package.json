{
  "name": "@xpfw/form-native",
  "version": "0.1.1",
  "main": "dist",
  "typings": "dist",
  "license": "MIT",
  "peerDependencies": {
    "@xpfw/form-shared": "0.1.x",
    "@xpfw/validate": "0.1.x",
    "lodash": "4.x.x",
    "moment": "2.x.x",
    "react-native": "0.x.x",
    "react-native-elements": "0.x.x",
    "resub": "1.x.x",
    "resub-persist": "1.x.x"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "dev": "./node_modules/.bin/rollup -c -w",
    "test": "./node_modules/.bin/jest --ci",
    "storybook": "./node_modules/.bin/start-storybook -p 9001 -c .storybook",
    "update": "ncu -a",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^11.11.6",
    "@types/react": "^16.8.8",
    "@types/react-native": "^0.57.41",
    "@xpfw/form-shared": "0.1.5",
    "@xpfw/form-tests": "0.1.1",
    "@xpfw/test-nativerender": "^0.1.1",
    "@xpfw/validate": "0.1.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-native": "^4.0.1",
    "jest": "^24.5.0",
    "jest-cli": "^24.5.0",
    "lodash": "^4.17.11",
    "mockdate": "^2.0.2",
    "moment": "^2.24.0",
    "react": "^16.8.5",
    "react-native": "^0.59.1",
    "react-native-elements": "^1.1.0",
    "react-native-vector-icons": "^6.4.2",
    "react-test-renderer": "^16.8.5",
    "regenerator-runtime": "^0.13.2",
    "resub": "^1.0.13",
    "resub-persist": "^1.2.8",
    "rollup": "^1.7.3",
    "rollup-plugin-typescript2": "^0.20.1",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "=24.0.0",
    "typescript": "^3.3.4000",
    "uglify-es": "^3.3.10"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "preset": "react-native",
    "transform": {
      ".(js|jsx)": "babel-jest",
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "preact-render-to-json": "@xpfw/test-nativerender",
      "\\.(css|sass|scss)$": "<rootDir>/__mocks__/styleMock.ts",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testURL": "http://localhost/",
    "transformIgnorePatterns": []
  }
}
