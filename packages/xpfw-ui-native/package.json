{
  "name": "@xpfw/ui-native",
  "version": "0.1.1",
  "main": "dist",
  "typings": "dist",
  "license": "MIT",
  "peerDependencies": {
    "@xpfw/form-shared": "0.1.x",
    "@xpfw/ui-shared": "0.1.x",
    "@xpfw/validate": "0.1.x",
    "lodash": "4.x.x",
    "moment": "2.x.x",
    "react-native": "0.x.x",
    "react-native-elements": "1.x.x"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "dev": "./node_modules/.bin/rollup -c -w",
    "test": "./node_modules/.bin/jest --ci",
    "update": "ncu -a",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^13.7.6",
    "@types/react": "^16.9.23",
    "@types/react-native": "^0.61.17",
    "@xpfw/form-native": "^0.1.1",
    "@xpfw/form-shared": "0.1.5",
    "@xpfw/test-nativerender": "^0.1.1",
    "@xpfw/test-util": "1.0.0",
    "@xpfw/ui-feathers": "^0.1.1",
    "@xpfw/ui-shared": "0.1.1",
    "@xpfw/ui-tests": "^0.1.1",
    "@xpfw/validate": "0.1.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^25.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-native": "^4.0.1",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "lodash": "^4.17.11",
    "mockdate": "^2.0.5",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-native": "^0.59.1",
    "react-native-elements": "^1.1.0",
    "react-native-vector-icons": "^6.6.0",
    "react-test-renderer": "^16.12.0",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^1.31.1",
    "rollup-plugin-typescript2": "^0.26.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "=25.2.1",
    "typescript": "^3.8.2",
    "uglify-es": "^3.3.10"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "preset": "react-native",
    "transform": {
      ".(js|jsx)": "babel-jest",
      ".(ts|tsx)": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "preact-render-to-json": "@xpfw/test-nativerender",
      "\\.(css|sass|scss)$": "<rootDir>/__mocks__/styleMock.ts",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testURL": "http://localhost/",
    "transformIgnorePatterns": []
  }
}
