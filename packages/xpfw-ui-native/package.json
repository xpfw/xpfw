{
  "name": "@xpfw/data-native",
  "version": "1.0.0",
  "main": "dist",
  "typings": "dist",
  "license": "MIT",
  "peerDependencies": {
    "@xpfw/data": "1.x.x",
    "@xpfw/form": "1.x.x",
    "ajv": "^6.x.x",
    "lodash": "^4.x.x",
    "mobx": "^5.x.x",
    "react": "^16.x.x",
    "react-native": "0.x.x",
    "react-native-elements": "1.x.x"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "dev": "./node_modules/.bin/rollup -c -w",
    "test": "./node_modules/.bin/jest --ci",
    "update": "ncu -a",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.6",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^13.7.6",
    "@types/react": "^16.9.23",
    "@types/react-native": "^0.61.17",
    "@xpfw/data": "^1.0.0",
    "@xpfw/data-feathers": "^1.0.0",
    "@xpfw/data-tests": "^1.0.0",
    "@xpfw/form": "^1.0.0",
    "@xpfw/form-native": "^1.0.0",
    "@xpfw/test-nativerender": "^0.1.1",
    "@xpfw/test-util": "^1.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^25.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-native": "^4.0.1",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "lodash": "^4.17.11",
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.8",
    "mockdate": "^2.0.5",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.13.0",
    "react-native": "^0.59.1",
    "react-native-elements": "^1.1.0",
    "react-native-vector-icons": "^6.6.0",
    "react-test-renderer": "^16.12.0",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^1.31.1",
    "rollup-plugin-typescript2": "^0.26.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "=25.2.1",
    "typescript": "^3.8.2",
    "uglify-es": "^3.3.10"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "preset": "react-native",
    "transform": {
      ".(js|jsx)": "babel-jest",
      ".(ts|tsx)": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "preact-render-to-json": "@xpfw/test-nativerender",
      "\\.(css|sass|scss)$": "<rootDir>/__mocks__/styleMock.ts",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
      "^react$": "<rootDir>/node_modules/react",
      "^react-native$": "<rootDir>/node_modules/react-native"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testURL": "http://localhost/",
    "transformIgnorePatterns": [
      "\\.json$",
      "node_modules/@xpfw/*"
    ]
  }
}
