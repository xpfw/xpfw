{
  "name": "@xpfw/form",
  "version": "1.0.0-beta.0",
  "main": "dist",
  "typings": "dist",
  "license": "MIT",
  "peerDependencies": {
    "ajv": "^6.x.x",
    "lodash-es": "^4.x.x",
    "mobx": "^5.x.x",
    "react": "^16.x.x"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "dev": "./node_modules/.bin/rollup -c -w",
    "test": "./node_modules/.bin/jest --ci",
    "update": "ncu -a",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/jest": "^24.0.4",
    "@types/lodash-es": "^4.17.2",
    "@types/mockdate": "^2.0.0",
    "@types/react": "^16.8.3",
    "ajv": "^6.9.1",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "lodash": "^4.17.11",
    "lodash-es": "^4.17.11",
    "mobx": "^5.9.0",
    "react": "^16.8.1",
    "rollup": "^1.1.2",
    "rollup-plugin-typescript2": "^0.19.2",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "=23.10.5",
    "tslib": "^1.9.3",
    "typescript": "^3.3.3",
    "uglify-es": "^3.3.10"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "^lodash-es$": "<rootDir>/node_modules/lodash/index.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testURL": "http://localhost/"
  }
}
