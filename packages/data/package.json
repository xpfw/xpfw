{
  "name": "@xpfw/data",
  "version": "1.0.0-beta.19",
  "main": "dist",
  "typings": "dist",
  "license": "MIT",
  "peerDependencies": {
    "@xpfw/form": "1.x.x",
    "ajv": "^6.x.x",
    "lodash-es": "^4.x.x",
    "mobx": "^5.x.x",
    "react": "^16.x.x",
    "fast-json-patch": "^2.1.x"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "dev": "./node_modules/.bin/rollup -c -w",
    "test": "./node_modules/.bin/jest --ci",
    "update": "ncu -a",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "fast-json-patch": "^2.0.7",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.134",
    "@types/lodash-es": "^4.17.3",
    "@types/mockdate": "^2.0.0",
    "@types/react": "^16.8.19",
    "@types/socket.io-client": "^1.4.32",
    "@xpfw/data-feathers": "1.0.0-beta.18",
    "@xpfw/data-tests": "1.0.0-beta.18",
    "@xpfw/form": "1.0.0-beta.18",
    "@xpfw/form-tests": "1.0.0-beta.18",
    "@xpfw/test-util": "1.0.0-beta.18",
    "ajv": "^6.10.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "lodash": "^4.17.11",
    "lodash-es": "^4.17.11",
    "mobx": "^5.9.0",
    "mockdate": "^2.0.2",
    "moment": "^2.24.0",
    "react": "^16.8.5",
    "react-test-renderer": "^16.8.6",
    "resub": "^1.0.13",
    "resub-persist": "^1.2.8",
    "rollup": "^1.15.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "=24.0.2",
    "typescript": "^3.5.1",
    "uglify-es": "^3.3.10"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "^@xpfw/ui-feathers$": "<rootDir>/node_modules/@xpfw/ui-feathers",
      "^@xpfw/data-feathers$": "<rootDir>/node_modules/@xpfw/data-feathers",
      "^@xpfw/data$": "<rootDir>/src",
      "^@xpfw/form-tests$": "<rootDir>/node_modules/@xpfw/form-tests",
      "^@xpfw/form$": "<rootDir>/node_modules/@xpfw/form",
      "^react$": "<rootDir>/node_modules/react",
      "^react-test-renderer$": "<rootDir>/node_modules/react-test-renderer",
      "^lodash-es$": "<rootDir>/node_modules/lodash/index.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testURL": "http://localhost/"
  }
}
