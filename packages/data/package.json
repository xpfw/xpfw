{
  "name": "@xpfw/data",
  "version": "0.2.0",
  "main": "dist",
  "typings": "dist",
  "license": "MIT",
  "peerDependencies": {
    "@xpfw/form": "0.2.x",
    "ajv": "^6.x.x",
    "lodash-es": "^4.x.x",
    "mobx": "^5.x.x",
    "react": "^16.x.x"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "dev": "./node_modules/.bin/rollup -c -w",
    "test": "./node_modules/.bin/jest --ci",
    "update": "ncu -a",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.118",
    "@types/lodash-es": "^4.17.2",
    "@types/mockdate": "^2.0.0",
    "@types/react": "^16.7.6",
    "@types/socket.io-client": "^1.4.32",
    "@xpfw/form": "0.2.0",
    "@xpfw/form-shared": "0.1.1",
    "@xpfw/test-util": "0.1.1",
    "@xpfw/data-feathers": "0.2.0",
    "@xpfw/data-tests": "0.2.0",
    "ajv": "^6.9.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "lodash": "^4.17.11",
    "lodash-es": "^4.17.11",
    "mobx": "^5.9.0",
    "mockdate": "^2.0.2",
    "moment": "^2.22.2",
    "preact": "^8.3.1",
    "preact-compat": "^3.18.4",
    "preact-render-to-json": "^3.6.6",
    "quick_check": "^0.7.0",
    "resub": "^1.0.13",
    "resub-persist": "^1.2.8",
    "rollup": "^0.67.3",
    "rollup-plugin-typescript2": "^0.18.0",
    "rollup-plugin-uglify": "^6.0.0",
    "ts-jest": "=23.10.4",
    "typescript": "^3.1.6",
    "uglify-es": "^3.3.10"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "^@xpfw/ui-feathers$": "<rootDir>/node_modules/@xpfw/ui-feathers",
      "^@xpfw/data-feathers$": "<rootDir>/node_modules/@xpfw/data-feathers",
      "^@xpfw/data$": "<rootDir>/src",
      "^@xpfw/form-tests$": "<rootDir>/node_modules/@xpfw/form-tests",
      "^@xpfw/form$": "<rootDir>/node_modules/@xpfw/form",
      "^preact-render-to-json$": "preact-render-to-json",
      "^preact$": "<rootDir>/node_modules/preact",
      "^react$": "<rootDir>/node_modules/preact-compat",
      "^react-dom$": "<rootDir>/node_modules/preact-compat",
      "^lodash-es$": "<rootDir>/node_modules/lodash/index.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testURL": "http://localhost/"
  }
}
