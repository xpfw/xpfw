{
  "name": "@xpfw/ui-feathers",
  "version": "0.1.1",
  "main": "dist",
  "typings": "dist",
  "license": "MIT",
  "dependencies": {
    "@feathersjs/authentication-client": "^1.0.8",
    "@feathersjs/feathers": "^3.2.3",
    "@feathersjs/rest-client": "^1.4.5",
    "@feathersjs/socketio-client": "^1.1.5",
    "resub-persist": "^1.2.8",
    "socket.io-client": "^2.1.1"
  },
  "peerDependencies": {
    "lodash": "4.x.x",
    "moment": "2.x.x",
    "@xpfw/validate": "0.1.x"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "dev": "./node_modules/.bin/rollup -c -w",
    "test": "./node_modules/.bin/jest --ci",
    "update": "ncu -a",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/feathersjs__authentication-client": "^1.0.4",
    "@types/feathersjs__feathers": "^3.0.5",
    "@types/feathersjs__rest-client": "^1.3.0",
    "@types/feathersjs__socketio-client": "^1.0.1",
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.118",
    "@types/mockdate": "^2.0.0",
    "@types/socket.io-client": "^1.4.32",
    "@xpfw/form-shared": "^0.1.1",
    "@xpfw/test-util": "0.1.1",
    "@xpfw/ui-shared": "^0.1.1",
    "@xpfw/ui-tests": "0.1.1",
    "@xpfw/validate": "^0.1.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "lodash": "^4.17.11",
    "mockdate": "^2.0.2",
    "moment": "^2.22.2",
    "react": "^16.6.3",
    "rollup": "^0.67.3",
    "rollup-plugin-typescript2": "^0.18.0",
    "rollup-plugin-uglify": "^6.0.0",
    "ts-jest": "=23.10.4",
    "typescript": "^3.1.6",
    "uglify-es": "^3.3.10"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "@xpfw/validate": "<rootDir>/node_modules/@xpfw/validate",
      "@xpfw/form-shared": "<rootDir>/node_modules/@xpfw/form-shared",
      "@xpfw/ui-shared": "<rootDir>/node_modules/@xpfw/ui-shared",
      "react": "<rootDir>/node_modules/react"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testURL": "http://localhost/"
  }
}
