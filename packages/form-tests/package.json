{
  "name": "@xpfw/form-tests",
  "version": "1.0.0-beta.12",
  "main": "dist",
  "typings": "dist",
  "license": "MIT",
  "peerDependencies": {
    "lodash-es": "^4.x.x",
    "moment": "2.x.x",
    "react": "^16.x.x",
    "@xpfw/form": "1.x.x"
  },
  "dependencies": {
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "dev": "./node_modules/.bin/rollup -c -w",
    "test": "./node_modules/.bin/jest --ci",
    "update": "ncu -a",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^24.0.4",
    "@types/lodash": "^4.14.121",
    "@types/lodash-es": "^4.17.2",
    "@types/mockdate": "^2.0.0",
    "@types/react": "^16.8.3",
    "@types/react-test-renderer": "^16.8.1",
    "@types/socket.io-client": "^1.4.32",
    "lodash-es": "^4.17.11",
    "moment": "^2.24.0",
    "@xpfw/form": "^1.0.0-beta.12",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "mockdate": "^2.0.2",
    "react": "^16.8.1",
    "react-test-renderer": "^16.8.2",
    "rollup": "^1.1.2",
    "rollup-plugin-typescript2": "^0.19.2",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "=23.10.5",
    "tslib": "^1.9.3",
    "typescript": "^3.3.3",
    "uglify-es": "^3.3.10"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "preact-render-to-json": "preact-render-to-json",
      "preact": "preact",
      "react": "preact-compat",
      "react-dom": "preact-compat"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testURL": "http://localhost/"
  }
}
